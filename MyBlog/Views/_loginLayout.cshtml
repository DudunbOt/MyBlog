<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0 shrink-to-fit=no">
    <meta name="description" content="" />
    <meta name="author" content="" />
    <title>Login Page</title>
    <link rel="icon" type="image/x-icon" href="~/Assets/favicon.ico" />
    @Styles.Render("https://fonts.googleapis.com/css?family=Lora:400,700,400italic,700italic")
    @Styles.Render("https://fonts.googleapis.com/css?family=Open+Sans:300italic,400italic,600italic,700italic,800italic,400,300,600,700,800")
    @Styles.Render("~/Content/styles.css")

    @Scripts.Render("https://use.fontawesome.com/releases/v6.1.0/js/all.js")
</head>
<body style="background: rgb(110 110 110);">
    <div class="container">
        <div class="row">
            <div class="col-sm-9 col-md-7 col-lg-5 mx-auto" style="margin-top:70px">
                <div class="card border-0 shadow rounded-3 my-5">
                    <div class="card-body p-4 p-sm-5">
                        <h5 class="card-title text-center mb-5 fw-light fs-5">Welcome Back!</h5>
                        @RenderBody()
                    </div>
                </div>
            </div>
        </div>
    </div>

    @Scripts.Render("https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js")
    @Scripts.Render("~/Scripts/scripts.js")
    @RenderSection("scripts", required: false)
</body>
</html>